<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnvironmentVariables</key>
	<dict>
		<key>RAILS_ENV</key>
		<string>production</string>
	</dict>
	<key>Label</key>
	<string>com.puppetdash.worker</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/www/puppet-dashboard/script/delayed_job</string>
		<string>-p</string>
		<string>dashboard</string>
		<string>-n</string>
		<string>-8</string>
		<string>-m</string>
		<string>start</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/var/log/puppetdash/puppetdash.log</string>
	<key>StandardOutPath</key>
	<string>/var/log/puppetdash/puppetdash.log</string>
</dict>
</plist>
